public static void main (String[] args) {

    System.out.println(20 / 0);
    
    System.out.println(“Fim”);
    
    }
    public static void main (String[] args) {

        try {
        
             System.out.println(20 / 0);
        
        } catch (Exception excecao) {
        
             System.out.println(Ocorreu o erro: “ + excecao.getMassage());
        
        }
        
             System.out.println(“Fim”);
        
        }
